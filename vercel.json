{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "VITE_FACEBOOK_APP_ID": "@facebook_app_id",
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}
